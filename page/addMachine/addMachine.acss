.videoContainer{
    width: 100%;
    height: 100vh;
    position: fixed;
    top: 0px;
    z-index: 999;

}

.video{
    width: 100%;
    height: 90vh;
    background-color: #333333;

}
.videoButton{
    height:10vh;
    /*position:fixed;*/
    width: 100%;
    margin: 0px;
}

.videoClose{
    width:100rpx;
    height:100rpx;
    border-radius:60%;
    position:absolute;
    top: 40rpx;
    left: 40rpx;
}

/* 将组件mask中的内容,覆盖全页 */
.mask .modal-content{ 
    margin-top:0;
}